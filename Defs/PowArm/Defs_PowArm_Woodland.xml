<Defs>
  <ThingDef ParentName="ArmorHelmetMakeableBase">
    <defName>WoodlandPowerArmorHelmet</defName>
    <label>marine helmet (Woodland)</label>
    <description>A marine armor helmet, with a built-in status computer and layered plasteel-weave plates.\n\nArmor like this is often used by imperial janissaries and rapid-incursion space marines.\n\nThis armour version has a woodland camouflage pattern.</description>
    <techLevel>Spacer</techLevel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <researchPrerequisite>PoweredArmor</researchPrerequisite>
      <skillRequirements>
        <Crafting>9</Crafting>
      </skillRequirements>
      <recipeUsers Inherit="False">
        <li>FabricationBench</li>
      </recipeUsers>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Item/PowArm/Woodland/Apparel/WoodlandPowerArmorHelmet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <generateCommonality>1</generateCommonality>
    <statBases>
      <WorkToMake>21500</WorkToMake>
      <MaxHitPoints>160</MaxHitPoints>
      <Mass>2.7</Mass>
      <Flammability>0.4</Flammability>
      <ArmorRating_Sharp>1.00</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.36</ArmorRating_Blunt>
      <ArmorRating_Heat>0.9</ArmorRating_Heat>
      <Insulation_Cold>4</Insulation_Cold>
      <Insulation_Heat>2</Insulation_Heat>
      <EquipDelay>4</EquipDelay>
    </statBases>
    <costList>
      <ComponentSpacer>1</ComponentSpacer>
      <Plasteel>40</Plasteel>
    </costList>
    <apparel>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Item/PowArm/Woodland/Head/WoodlandPowerArmorHelmet</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>PassiveCamo_Woodland_High</li>
        <li>SpacerMilitary</li>
        <li>RoyalCombatGear</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
      <useDeflectMetalEffect>true</useDeflectMetalEffect>
    </apparel>
    <thingSetMakerTags>
      <li>RewardStandardHighFreq</li>
    </thingSetMakerTags>
    <thingCategories Inherit="false">
      <li>CASHeadgear</li>
