<Defs>
  <ThingDef ParentName="ApparelMakeableBase" Name="CASPowBootsBase" Abstract="true">
    <defName>CASPowerArmBoots</defName>
    <label>marine boots</label>
    <description>A pair of marine boots with layered plasteel plates and enhanced servomotors.</description>
    <techLevel>Spacer</techLevel>
    <tradeability>Sellable</tradeability>
    <generateCommonality>0.05</generateCommonality>
    <costList>
      <Hyperweave>5</Hyperweave>
      <Plasteel>20</Plasteel>
      <ComponentSpacer>1</ComponentSpacer>
    </costList>
    <statBases>
      <MaxHitPoints>160</MaxHitPoints>
      <WorkToMake>16000</WorkToMake>
      <Mass>2</Mass>
      <Flammability>0.4</Flammability>
      <ArmorRating_Sharp>0.74</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.3</ArmorRating_Blunt>
      <ArmorRating_Heat>0.7</ArmorRating_Heat>
      <Insulation_Cold>2</Insulation_Cold>
      <Insulation_Heat>4</Insulation_Heat>
      <EquipDelay>2.5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>0.1</MoveSpeed>
    </equippedStatOffsets>
    <thingCategories Inherit="False">
      <li>CASFootWear</li>
    </thingCategories>
    <apparel>
      <bodyPartGroups>
        <li>Feet</li>
      </bodyPartGroups>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
        <li>SpacerMilitary</li>
        <li>RoyalCombatGear</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <researchPrerequisite>PoweredArmor</researchPrerequisite>
      <skillRequirements>
        <Crafting>8</Crafting>
      </skillRequirements>
      <recipeUsers Inherit="False">
        <li>FabricationBench</li>
      </recipeUsers>
    </recipeMaker>
    <thingSetMakerTags>
      <li>RewardStandardHighFreq</li>
    </thingSetMakerTags>
    <tradeTags>
      <li>HiTechArmor</li>
    </tradeTags>
  </ThingDef>
